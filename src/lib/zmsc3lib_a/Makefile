# Makefile for zmsc3lib.a

AS = has060
ASFLAGS = -w2 -c2 -i../ -i../../ -i../../include
HAR = har
HARFLAGS = -u -n

SRCS = $(wildcard *.S)
OBJS = $(SRCS:.S=.O)
lower_o = GETZMLIBVER.o CHECKZMSC.o ZMZMDSE.o ZM_PLAY_all.o

all: ../ZMSC3LIB.A

%.o: %.s
%.O: %.S
	$(AS) $(ASFLAGS) -o$@ $<

%.o: %.O
	cp -f $< $@

ZM_PLAY_all.o: ZM_PLAY_ALL.S
	cp -f $< ZM_PLAY_ZMD.has
	$(AS) $(ASFLAGS) -o$@ ZM_PLAY_ZMD.has

